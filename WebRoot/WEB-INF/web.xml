<?xml version="1.0" encoding="UTF-8"?>
<web-app version="3.0" 
	xmlns="http://java.sun.com/xml/ns/javaee" 
	xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance" 
	xsi:schemaLocation="http://java.sun.com/xml/ns/javaee 
	http://java.sun.com/xml/ns/javaee/web-app_3_0.xsd">
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>HelloWorld</servlet-name>
    <servlet-class>HelloWorld</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>Three</servlet-name>
    <servlet-class>Three</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>SetCookies</servlet-name>
    <servlet-class>SetCookies</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowCookies</servlet-name>
    <servlet-class>ShowCookies</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowRs</servlet-name>
    <servlet-class>ShowRs</servlet-class>
  </servlet>
  <servlet>
    <description>This is the description of my J2EE component</description>
    <display-name>This is the display name of my J2EE component</display-name>
    <servlet-name>ShowRsUseBean</servlet-name>
    <servlet-class>ShowRsUseBean</servlet-class>
  </servlet>






  <servlet-mapping>
    <servlet-name>HelloWorld</servlet-name>
    <url-pattern>/HelloWorld</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>Three</servlet-name>
    <url-pattern>/servlet/Three</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>SetCookies</servlet-name>
    <url-pattern>/servlet/SetCookies</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowCookies</servlet-name>
    <url-pattern>/servlet/ShowCookies</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowRs</servlet-name>
    <url-pattern>/servlet/ShowRs</url-pattern>
  </servlet-mapping>
  <servlet-mapping>
    <servlet-name>ShowRsUseBean</servlet-name>
    <url-pattern>/servlet/ShowRsUseBean</url-pattern>
  </servlet-mapping>

</web-app>
